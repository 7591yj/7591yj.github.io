---
import { type CollectionEntry, getCollection, render } from "astro:content";
import BlogPost from "../../layouts/SoftwareDevelopmentPost.astro";

export async function getStaticPaths() {
  const posts = await getCollection("softwareDevelopment");
  return posts.map((post) => ({
    params: { slug: post.id },
    props: post,
  }));
}
type Props = CollectionEntry<"softwareDevelopment">;

const post = Astro.props;
const { Content } = await render(post);
---

<BlogPost {...post.data}>
  <Content />
</BlogPost>
