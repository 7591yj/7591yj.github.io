---
import Header from "../components/Header.astro";
import { NAME } from "../consts.ts";
---

<!doctype html>
<html>
  <Header title=`404 Error | ${NAME}` />
  <body>
    <div class="content">
      <nav class="main">
        <div class="container">
          <div class="back-nav-container">
            <a href="/" class="back-nav-button w-inline-block"
              ><img
                src="/assets/back.svg"
                loading="lazy"
                alt=""
                class="back-nav-img"
              />
              <div>Back</div>
            </a>
          </div>
          <div class="content-container">
            <h1>404 Error</h1>
            <p>How did you end up here?</p>
          </div>
        </div>
      </nav>
    </div>

    <!--[if lte IE 9]><script src="//cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif]-->
  </body>
</html>
